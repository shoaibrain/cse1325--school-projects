#ifndef __PET_H
#define __PET_H


#include <iostream>
#include <string>

using namespace std;

class Pet{
    public:
        Pet(string _name, double _age,type _type)     //Constructor
        ~Pet();                          //Destructor
        
    private:
       string _name;
       double _age;
       type _type;
        
}



#endif
